def main():
    x = 42
    y = 3.14
    print(x + y)
    
    if x > 40:
        print(x * 2)
    else:
        print(x / 2)

def fibonacci(n):
    if n <= 1:
        return n
    return fibonacci(n - 1) + fibonacci(n - 2)

def factorial(n):
    if n <= 1:
        return 1
    return n * factorial(n - 1)