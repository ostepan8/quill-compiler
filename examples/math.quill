def add(a, b):
    return a + b

def multiply(a, b):
    return a * b

def power(base, exp):
    if exp == 0:
        return 1
    return base * power(base, exp - 1)

def main():
    x = 5
    y = 3
    
    sum_result = add(x, y)
    print(sum_result)
    
    product = multiply(x, y)
    print(product)
    
    power_result = power(2, 4)
    print(power_result)